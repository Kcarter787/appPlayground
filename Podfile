# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'AppPlayground' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AppPlayground
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'

  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'

  pod 'Batch', '~> 1.7'

  pod 'Alamofire', '~> 4.2.0'

  target 'AppPlaygroundTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
  end

  target 'AppPlaygroundUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'

  end

end
